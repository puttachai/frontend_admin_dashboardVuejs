                        pro_activity_id: activityId,
                        st: item.st, // ‚úÖ ‡πÄ‡∏Å‡πá‡∏ö‡∏Ñ‡πà‡∏≤ st ‡πÑ‡∏ß‡πâ‡∏î‡πâ‡∏ß‡∏¢
